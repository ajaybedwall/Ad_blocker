{
  "manifest_version": 3,
  "name": "Advanced Ad Blocker with Counter",
  "version": "1.1",
  "description": "Blocks ads on webpages and displays a count.",
  "permissions": ["activeTab", "scripting"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["style.css"]
    }
  ]
}
